{
  "name": "quester",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "npm run dev",
    "build": "tsc && vite build",
    "test": "jest",
    "test:watch": "jest --watchAll",
